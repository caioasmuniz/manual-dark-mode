{
  "manifest_version": 2,
  "name": "Manual Dark Mode",
  "version": "1",
  "description": "Aplica modo escuro em paginas que nao o suportam nativamente",
  "icons": {},
  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "manual-dark-mode",
    "default_popup": "popup/pannel.html"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}