{
  "hosting": {
    "public": "./website/Site_projeto",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"      
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "./website/Site_projeto/index.html"
      }
    ]
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
